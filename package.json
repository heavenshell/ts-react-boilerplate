  "scripts": {
    "build": "webpack --mode production",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged",
    "start": "webpack-dev-server --mode development --open --history-api-fallback",
    "storybook": "start-storybook -p 6006",
    "lint:ts": "eslint 'src/**/*.ts{,x}'",
    "lint:md": "markdownlint README.md",
    "lint": "run-p -l lint:*",
    "test": "jest --maxWorkers=1",
    "screenshot": "storycap --serverCmd 'start-storybook -p 6006' --serverTimeout 100000 http://localhost:6006 -i '**/regression'",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./configs/jest.setup.ts"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.spec.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/configs/tsconfig.test.json"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
